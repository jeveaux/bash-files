[user]
	name = jeveaux
	email = paulo.jeveaux@giran.com.br
[github]
	user = jeveaux
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[alias]
	ci = commit
	st = status
	br = branch
	co = checkout
	df = diff
	lg = log -p
	who = shortlog -s --
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[merge]
       ff = false

